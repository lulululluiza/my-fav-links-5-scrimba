{
    "manifest_version": 3,
    "version": "2.0",
    "name": "My Fav Links",
    "action": {
        "default_popup": "index.html",
        "default_icon": "icon.png"
    },
    "icons": 
    {
      "128": "icon.png"
    },
    "description": "Quickly save your fav tabs/links as you surf the web ;)",
    "permissions": [
        "tabs"
    ]
}